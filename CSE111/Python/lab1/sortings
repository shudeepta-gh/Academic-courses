#Bubble sort
given=[33,45,99,21]
for i in range(len(given)-1):
  for j in range(len(given)-i-1):
    if(given[j]>given[j+1]):
      temp = given[j]
      given[j] = given[j+1]
      given[j+1] = temp

print(given)

#Selection sort

given=[33,45,99,21]
for i in range(len(given)-1):
  min_pos=i
  for j in range(i+1,len(given)):
    if given[j]<given[i]:
      min_pos=j
  temp=given[i]
  given[i]=given[min_pos]
  given[min_pos]=temp
print(given)





